{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "jquery": true
    },
    "globals": {
        "L": true,
        "Papa": true,
        "vex": true
    },
    "rules": {
        "comma-dangle": ["off"],
        "indent": ["error", 4],
        "no-console": ["off"],
        "prefer-destructuring": ["off"],
        "quote-props": ["off"],
        "template-curly-spacing": ["error", "always"]
    }
}
